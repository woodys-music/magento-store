<!-- jQuery 1.4.3 -->
<action method="addJs" ifconfig="fisheyeall/js/jquery"><script>fisheye/all/jquery/jquery-1.4.3-min.js</script></action>


<!-- jQuery 1.6.1 -->
<action method="addJs" ifconfig="fisheyeall/js/jquery6"><script>fisheye/all/jquery/jquery-1.6.1-min.js</script></action>

<!-- Colorbox -->
